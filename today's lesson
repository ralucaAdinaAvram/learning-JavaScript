//===============     do/while loop    ======================

/*   */
 var number2 =0;
 do{
     
     document.write("8")
    document.write(" x ");
    document.write(number2);
    document.write(" = ");
    document.write(8 * number2);
    document.write("<br/>");


    number2++;
} while (number2<=10);



